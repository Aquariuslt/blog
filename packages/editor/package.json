    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "slate": "0.62.1",
    "slate-history": "0.62.0",
    "slate-react": "0.62.0",
    "typeface-roboto": "1.1.13"
  },
  "devDependencies": {
    "@testing-library/react": "11.2.6",
    "@types/gulp": "4.0.8",
    "@types/jest": "26.0.23",
    "@types/node": "13.13.51",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack": "5.28.0",
    "autoprefixer": "10.2.5",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.2.4",
